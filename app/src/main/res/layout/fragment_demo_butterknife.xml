<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.CardView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_gravity="center"
        android:layout_margin="12dp"
        android:id="@+id/cardView">

    </android.support.v7.widget.CardView>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button"/>

    <com.rkzk.android.bblmaterialdesign.customviews.RevealView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/revealView"/>

    <com.rkzk.android.bblmaterialdesign.customviews.NetWorkImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/networkImage"/>

    <com.rkzk.android.bblmaterialdesign.customviews.CustomSquareImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/suareImage" />

    <com.rkzk.android.bblmaterialdesign.customviews.RippleLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rippleLayout"/>

    <ExpandableListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/expandableList"/>

    <AnalogClock
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/analogClock"/>

</FrameLayout>
